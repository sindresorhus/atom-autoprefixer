{
	"name": "autoprefixer",
	"version": "4.2.0",
	"description": "Prefix CSS and SCSS",
	"license": "MIT",
	"repository": "sindresorhus/atom-autoprefixer",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "sindresorhus.com"
	},
	"private": true,
	"engines": {
		"atom": ">=1.36.0"
	},
	"scripts": {
		"test": "xo"
	},
	"keywords": [
		"css",
		"scss",
		"prefix",
		"prefixer"
	],
	"dependencies": {
		"autoprefixer": "^10.3.7",
		"postcss": "^8.3.9",
		"postcss-safe-parser": "^6.0.0",
		"postcss-scss": "^4.0.1",
		"postcss-unprefix": "^2.1.4"
	},
	"devDependencies": {
		"xo": "^0.24.0"
	},
	"activationCommands": {
		"atom-workspace": [
			"core:save",
			"autoprefixer:prefix",
			"autoprefixer:remove-prefixes"
		]
	},
	"xo": {
		"globals": [
			"atom"
		]
	}
}
